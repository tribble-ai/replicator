{
  "name": "@tribble/sdk-policy",
  "version": "0.1.0",
  "description": "Tribble SDK Policy Engine - Extension validation and security policies",
  "private": false,
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --sourcemap --clean"
  },
  "dependencies": {
    "@tribble/sdk-extensions": "*",
    "zod": "^3.23.0"
  }
}
