{
  "name": "@tribble/sdk",
  "version": "0.1.0",
  "description": "Tribble Platform SDK - Build GenAI applications with document ingestion, agent chat, and workflow automation",
  "private": false,
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./agent": {
      "types": "./dist/agent.d.ts",
      "import": "./dist/agent.mjs",
      "require": "./dist/agent.cjs"
    },
    "./ingest": {
      "types": "./dist/ingest.d.ts",
      "import": "./dist/ingest.mjs",
      "require": "./dist/ingest.cjs"
    },
    "./workflows": {
      "types": "./dist/workflows.d.ts",
      "import": "./dist/workflows.mjs",
      "require": "./dist/workflows.cjs"
    },
    "./events": {
      "types": "./dist/events.d.ts",
      "import": "./dist/events.mjs",
      "require": "./dist/events.cjs"
    },
    "./auth": {
      "types": "./dist/auth.d.ts",
      "import": "./dist/auth.mjs",
      "require": "./dist/auth.cjs"
    },
    "./queue": {
      "types": "./dist/queue.d.ts",
      "import": "./dist/queue.mjs",
      "require": "./dist/queue.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts src/agent.ts src/ingest.ts src/workflows.ts src/events.ts src/auth.ts src/queue.ts --format esm,cjs --dts --sourcemap --clean"
  },
  "dependencies": {
    "@tribble/sdk-core": "*",
    "@tribble/sdk-agent": "*",
    "@tribble/sdk-ingest": "*",
    "@tribble/sdk-workflows": "*",
    "@tribble/sdk-events": "*",
    "@tribble/sdk-auth": "*",
    "@tribble/sdk-queue": "*"
  }
}
