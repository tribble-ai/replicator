{
  "name": "@tribble/sdk",
  "version": "0.2.0",
  "description": "Tribble Platform SDK - Build production GenAI applications with offline support, structured output, and observability",
  "private": false,
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts src/react.ts --format esm,cjs --dts --sourcemap --clean --external react --external react-dom"
  },
  "dependencies": {
    "@tribble/sdk-core": "*",
    "@tribble/sdk-agent": "*",
    "@tribble/sdk-ingest": "*",
    "@tribble/sdk-workflows": "*",
    "@tribble/sdk-events": "*",
    "@tribble/sdk-auth": "*",
    "@tribble/sdk-queue": "*",
    "@tribble/sdk-structured": "*",
    "@tribble/sdk-offline": "*",
    "@tribble/sdk-conversations": "*",
    "@tribble/sdk-telemetry": "*",
    "@tribble/sdk-prompts": "*",
    "@tribble/sdk-batch": "*"
  },
  "peerDependencies": {
    "zod": "^3.22.0"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  }
}
