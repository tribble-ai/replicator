{
  "name": "@tribble/sdk-sap",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "sideEffects": false,
  "description": "SAP S/4HANA deployment module for Tribble SDK - Fiori apps, OData services, and ABAP integration",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./ui5": {
      "types": "./dist/ui5/index.d.ts",
      "import": "./dist/ui5/index.mjs",
      "require": "./dist/ui5/index.cjs"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.mjs",
      "require": "./dist/cli/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "tribble-sap": "./dist/cli/index.cjs"
  },
  "files": [
    "dist",
    "templates",
    "abap"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/ui5/index.ts src/cli/index.ts --format esm,cjs --dts --sourcemap --clean"
  },
  "keywords": [
    "tribble",
    "sap",
    "s4hana",
    "fiori",
    "ui5",
    "odata",
    "abap",
    "btp",
    "launchpad"
  ],
  "dependencies": {
    "@tribble/sdk-core": "workspace:*",
    "@tribble/sdk-agent": "workspace:*",
    "@tribble/sdk-chat": "workspace:*",
    "@tribble/sdk-ingest": "workspace:*",
    "@tribble/sdk-auth": "workspace:*",
    "commander": "^12.1.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "prompts": "^2.4.2",
    "xml2js": "^0.6.2",
    "archiver": "^7.0.1",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/xml2js": "^0.4.14",
    "@types/archiver": "^6.0.2",
    "@types/prompts": "^2.4.9",
    "@sapui5/types": "^1.120.0"
  }
}
