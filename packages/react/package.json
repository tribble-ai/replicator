{
  "name": "@tribble/sdk-react",
  "version": "0.1.0",
  "description": "React components for Tribble SDK - Chat, Insights, and Data Display",
  "private": false,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --sourcemap --clean --external react --external react-dom"
  },
  "dependencies": {
    "@tribble/sdk-agent": "*",
    "@tribble/sdk-conversations": "*",
    "@tribble/sdk-offline": "*",
    "@tribble/sdk-structured": "*"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "zod": "^3.22.0"
  }
}
