{
  "name": "exceedra-integration-example",
  "version": "1.0.0",
  "description": "Production-ready exceedra REST API integration using Tribble SDK",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "sync": "tsx src/index.ts sync",
    "sync:full": "tsx src/index.ts sync --full",
    "sync:incremental": "tsx src/index.ts sync --incremental",
    "validate": "tsx src/index.ts validate",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "format": "prettier --write src"
  },
  "dependencies": {
    "@tribble/sdk-core": "*",
    "@tribble/sdk-ingest": "*",
    "@tribble/sdk-integrations": "*",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "tribble",
    "exceedra",
    "integration",
    "rest-api",
    "data-sync"
  ]
}
